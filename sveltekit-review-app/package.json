{
	"name": "sveltekit-review-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build:ssg": "rimraf build-static && node scripts/run-sveltekit.cjs sync && vite build",
		"postbuild:ssg": "node scripts/copy-static.cjs && node scripts/zip-mf-types.cjs",
		"build": "npm run build:ssg && npm run postbuild:ssg",
		"dev": "svelte-kit dev",
		"preview": "svelte-kit preview",
		"mf:generate-types": "tsc --project tsconfig-mf.json && node scripts/zip-mf-types.cjs",
		"start": "npm run mf:generate-types && vite --config vite.mf.config.ts",
		"mf:dev": "vite --config vite.mf.config.ts",
		"mf:build": "npm run mf:generate-types && vite build --config vite.mf.config.ts",
		"mf:preview": "vite preview --config vite.mf.config.ts --port 5174",
		"postinstall": "svelte-kit sync"
	},
	"devDependencies": {
		"@module-federation/vite": "^1.8.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.40.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.12.2",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.4.0",
		"rimraf": "^5.0.5",
		"svelte": "^5.38.10",
		"svelte-check": "^4.3.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.20.0",
		"vite": "^5.4.10",
		"zip-a-folder": "^3.1.9"
	},
	"dependencies": {
		"@emotion/css": "^11.13.5"
	},
	"overrides": {
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"svelte": "^5.38.10"
	}
}