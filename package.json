{
  "name": "root",
  "private": true,
  "workspaces": [
    "main-container",
    "navigation-bar-app",
    "vue-account-app",
    "vue-ai-interview-app",
    "studyroom-app",
    "packages/*",
    "mypage-app",
    "spoon-word-app",
    "sveltekit-review-app"
  ],
  "scripts": {
    "build:seo": "lerna run build --scope sveltekit-review-app --stream",
    "build:remotes": "lerna run build --stream --parallel --scope navigation-bar-app --scope vue-account-app --scope vue-ai-interview-app --scope studyroom-app --scope mypage-app --scope spoon-word-app",
    "build:host": "lerna run build --scope main-container --stream",
    "build": "npm run build:seo && npm run build:remotes && npm run build:host",
    "start": "lerna run start --parallel"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "external-remotes-plugin": "^1.0.0",
    "lerna": "^8.2.3",
    "postcss": "^8.5.6",
    "postcss-load-config": "^5.1.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^17.2.2",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "^18.2.0"
  },
  "overrides": {
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "svelte": "^5.38.10"
  }
}
